.goods-view{
    width: 100%;
    height: 90%;
}

.cars-view{
     /* height: 400rpx; */
    width: 95%;
    padding-left: 20rpx;
    padding-right: 20rpx;
    display: flex;
    flex-direction: column;
}

.cart-cell{
    background-color: white;
    height: 300rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
}


.cart-seleted-icon{
    padding-left: 20rpx;
    width: 50rpx;
    height: 50rpx;
}

.cart-cell-image{
    padding-left: 10rpx;
    width: 200rpx;
    height: 90%;
}

.cart-cell-info{
    padding-left: 10rpx;
    width: 100%;
    height: 90%;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
}


.cart-cell-info-price{
    padding-bottom: 10rpx !important;
    width: 100%;
    height: 30%;
    display: flex;
    flex-direction: row;
}

.price-view{
height: 9%;
width: 100%;
background-color: white;
display: flex;
flex-direction: row;
align-items: center;
}
.price-price-view{
height: 100%;
padding-left: 30rpx;
display: flex;
flex-direction: column;
width: 36.5%;
align-items: flex-end;
justify-content: flex-start;

}
